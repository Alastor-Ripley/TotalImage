<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net48;netcoreapp3.1</TargetFrameworks>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <PropertyGroup>
    <RootNamespace>TotalImage</RootNamespace>
    <AssemblyName>TotalImage</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationIcon>totalimage.ico</ApplicationIcon>
    <StartupObject>TotalImage.Program</StartupObject>
  </PropertyGroup>

</Project>